{
  "$schema": "https://schemas.wp.org/wp/6.3/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "layout": {
      "contentSize": "1440px",
      "wideSize": "1440px"
    },
    "spacing": {
      "units": ["%", "px", "em", "rem", "vh", "vw"]
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
          "name": "System Font",
          "slug": "system-font"
        }
      ]
    },
    "color": {
      "palette": [
        { "slug": "primary", "name": "Primary", "color": "#1e40af" },
        { "slug": "contrast", "name": "Contrast", "color": "#ffffff" },
        { "slug": "background", "name": "Background", "color": "#f5f5f5" },
        { "slug": "text", "name": "Text", "color": "#111111" },
        { "slug": "black", "name": "Black", "color": "#000000" },
        { "slug": "white", "name": "White", "color": "#ffffff" },
        { "slug": "light-green-cyan", "name": "Light Green Cyan", "color": "#7bdcb5" }
      ]
    },
    "useRootPaddingAwareAlignments": true
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header"
    },
    {
      "area": "footer",
      "name": "footer"
    }
  ]
}
