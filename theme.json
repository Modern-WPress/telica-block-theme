{
  "$schema": "https://schemas.wp.org/wp/6.3/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "layout": {
      "contentSize": "1440px",
      "wideSize": "1440px"
    },
    "spacing": {
      "units": ["%", "px", "em", "rem", "vh", "vw"]
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "Open Sans, sans-serif",
          "slug": "open-sans",
          "name": "Open Sans",
          "fontFace": [
            {
              "fontFamily": "Open Sans",
              "fontWeight": "300",
              "fontStyle": "normal",
              "src": [
                "file:./assets/fonts/open-sans/OpenSans-Light.woff2",
                "file:./assets/fonts/open-sans/OpenSans-Light.woff"
              ]
            },
            {
              "fontFamily": "Open Sans",
              "fontWeight": "400",
              "fontStyle": "normal",
              "src": [
                "file:./assets/fonts/open-sans/OpenSans-Regular.woff2",
                "file:./assets/fonts/open-sans/OpenSans-Regular.woff"
              ]
            },
            {
              "fontFamily": "Open Sans",
              "fontWeight": "500",
              "fontStyle": "normal",
              "src": [
                "file:./assets/fonts/open-sans/OpenSans-Medium.woff2",
                "file:./assets/fonts/open-sans/OpenSans-Medium.woff"
              ]
            },
            {
              "fontFamily": "Open Sans",
              "fontWeight": "600",
              "fontStyle": "normal",
              "src": [
                "file:./assets/fonts/open-sans/OpenSans-SemiBold.woff2",
                "file:./assets/fonts/open-sans/OpenSans-SemiBold.woff"
              ]
            },
            {
              "fontFamily": "Open Sans",
              "fontWeight": "800",
              "fontStyle": "normal",
              "src": [
                "file:./assets/fonts/open-sans/OpenSans-ExtraBold.woff2",
                "file:./assets/fonts/open-sans/OpenSans-ExtraBold.woff"
              ]
            }
          ]
        }
      ]
    },
    "color": {
      "palette": [
        { "slug": "primary", "name": "Primary", "color": "#1e40af" },
        { "slug": "contrast", "name": "Contrast", "color": "#ffffff" },
        { "slug": "background", "name": "Background", "color": "#f5f5f5" },
        { "slug": "text", "name": "Text", "color": "#111111" },
        { "slug": "black", "name": "Black", "color": "#000000" },
        { "slug": "white", "name": "White", "color": "#ffffff" },
        {
          "slug": "light-green-cyan",
          "name": "Light Green Cyan",
          "color": "#7bdcb5"
        }
      ]
    },
    "useRootPaddingAwareAlignments": true
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header"
    },
    {
      "area": "footer",
      "name": "footer"
    }
  ]
}
